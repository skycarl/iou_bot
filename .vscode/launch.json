    "version": "0.2.0",
    "configurations": [
        
        {
            "name": "Python: Current File",
            "type": "python",
            "request": "launch",
            "cwd": "${workspaceFolder}",
            "program": "${file}",
            "console": "integratedTerminal",
            "justMyCode": true
        },
        {
            "name": "IOU Bot",
            "type": "python",
            "request": "launch",
            "module": "app.main",
            "env": {
                "TELEGRAM_BOT_TOKEN": "token here",
                "X_TOKEN": "12345678910",
                "ENV": "dev",
                "APP_URL": "http://localhost:8000/api"
            },
            "console": "integratedTerminal",
            "justMyCode": true
        }
    ]
}